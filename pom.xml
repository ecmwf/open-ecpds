<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>ecmwf</groupId>
	<artifactId>product-data-store</artifactId>
	<version>6.7.7</version>
	<name>Product Data Store (PDS)</name>
	<description>Product Data Store for Acquisition and Dissemination activities</description>
	<url>
		https://www.ecmwf.int/en/newsletter/159/computing/ecmwf-production-data-store</url>
	<packaging>jar</packaging>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<maven.compiler.target>22</maven.compiler.target>
		<maven.compiler.source>22</maven.compiler.source>
		<java.home>${java.home}</java.home>
		<graalvm.version>24.0.1</graalvm.version>
		<packager.id>syi</packager.id>
		<packager.name>Laurent Gougeon</packager.name>
		<packager.email>laurent.gougeon@ecmwf.int</packager.email>
		<packager.distribution>OpenPDS</packager.distribution>
		<build.number>14052024</build.number>
	</properties>
	<licenses>
		<license>
			<name>Apache License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<scm>
		<connection>scm:git:git@github.com:ecmwf/openpds.git</connection>
		<developerConnection>scm:git:ssh@github.com:ecmwf/openpds.git</developerConnection>
		<url>https://github.com/ecmwf/openpds.git</url>
		<tag>HEAD</tag>
	</scm>
	<developers>
		<developer>
			<id>${packager.id}</id>
			<name>${packager.name}</name>
			<email>${packager.email}</email>
			<organization>ECMWF</organization>
			<organizationUrl>https://www.ecmwf.int</organizationUrl>
		</developer>
		<developer>
			<id>mabc</id>
			<name>Cristina-Iulia Bucur</name>
			<email>cristina-iulia.bucur@ecmwf.int</email>
			<organization>ECMWF</organization>
			<organizationUrl>https://www.ecmwf.int</organizationUrl>
		</developer>
		<developer>
			<id>sy8</id>
			<name>Daniel Varela Santoalla</name>
			<email>daniel.varela@ecmwf.int</email>
			<organization>ECMWF</organization>
			<organizationUrl>https://www.ecmwf.int</organizationUrl>
		</developer>
	</developers>
	<distributionManagement>
		<repository>
			<id>github</id>
			<name>GitHub Packages</name>
			<url>https://maven.pkg.github.com/ecmwf/openpds</url>
		</repository>
		<snapshotRepository>
			<id>github-snapshots</id>
			<name>GitHub Packages Snapshots</name>
			<url>https://maven.pkg.github.com/ecmwf/openpds</url>
		</snapshotRepository>
	</distributionManagement>
	<build>
		<finalName>ecmwf-${project.version}-${build.number}</finalName>
		<resources>
			<resource>
				<directory>src/main/java</directory>
				<includes>
					<include>ecmwf/common/version/Version.java</include>
				</includes>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>3.4.1</version>
				<executions>
					<execution>
						<id>compile-native</id>
						<goals>
							<goal>exec</goal>
						</goals>
						<phase>package</phase>
					</execution>
				</executions>
				<configuration>
					<executable>gcc</executable>
					<arguments>
						<argument>-shared</argument>
						<argument>-O2</argument>
						<argument>-D_GNU_SOURCE</argument>
						<argument>-D_FILE_OFFSET_BITS=64</argument>
						<argument>-olibsocketoptions.so</argument>
						<argument>-I${java.home}/include</argument>
						<argument>-I${java.home}/include/linux</argument>
						<argument>-fPIC</argument>
						<argument>${project.basedir}/src/main/c/SocketOptions.c</argument>
					</arguments>
					<workingDirectory>${project.build.directory}</workingDirectory>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>3.2.0</version>
				<executions>
					<execution>
						<id>process-resources</id>
						<phase>process-resources</phase>
						<goals>
							<goal>resources</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>3.4.2</version>
				<executions>
					<execution>
						<id>default-jar</id>
						<goals>
							<goal>jar</goal>
						</goals>
						<phase>package</phase>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>3.4.0</version>
				<configuration>
					<warName>${finalName}</warName>
					<packagingExcludes>WEB-INF/lib/*.jar</packagingExcludes>
				</configuration>
				<executions>
					<execution>
						<id>default-war</id>
						<goals>
							<goal>war</goal>
						</goals>
						<phase>package</phase>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.13.0</version>
				<configuration>
					<source>${maven.compiler.target}</source>
					<target>${maven.compiler.target}</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>3.3.1</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar-no-fork</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>3.8.0</version>
				<executions>
					<execution>
						<id>javadoc-jar</id>
						<phase>package</phase>
						<goals>
							<goal>jar</goal>
						</goals>
						<configuration>
							<includeDependencySources>false</includeDependencySources>
							<dependencySourceIncludes>
								<dependencySourceInclude>ecmwf:*</dependencySourceInclude>
							</dependencySourceIncludes>
							<detectJavaApiLink>false</detectJavaApiLink>
							<doclint>none</doclint>
							<source>${maven.compiler.target}</source>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>license-maven-plugin</artifactId>
				<version>2.4.0</version>
				<configuration>
					<licenseName>apache_v2</licenseName>
					<organizationName>ECMWF</organizationName>
					<inceptionYear>2004</inceptionYear>
					<includes>
						<include>**/*.java</include>
					</includes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>net.revelc.code.formatter</groupId>
				<artifactId>formatter-maven-plugin</artifactId>
				<version>2.24.1</version>
				<executions>
					<execution>
						<goals>
							<goal>format</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>copy-dependencies</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>
								${project.basedir}/third-party</outputDirectory>
							<includeScope>runtime</includeScope>
							<overWriteReleases>true</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
							<overWriteIfNewer>true</overWriteIfNewer>
							<excludeGroupIds>ecmwf</excludeGroupIds>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.eclipse.jetty</groupId>
				<artifactId>jetty-jspc-maven-plugin</artifactId>
				<version>10.0.18</version>
				<executions>
					<execution>
						<id>jspc</id>
						<goals>
							<goal>jspc</goal>
						</goals>
						<configuration>
							<webAppSourceDirectory>
								${project.basedir}/src/main/webapp</webAppSourceDirectory>
							<generatedClasses>
								${project.build.directory}/generated-sources/jsp</generatedClasses>
							<webXmlFragment>
								${project.build.directory}/generated-web.xml</webXmlFragment>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>rpm-maven-plugin</artifactId>
				<version>2.3.0</version>
				<executions>
					<execution>
						<id>rpm-ecpds-master</id>
						<phase>package</phase>
						<goals>
							<goal>rpm</goal>
						</goals>
						<configuration>
							<name>ecpds-master</name>
							<summary>Master-Server</summary>
							<description>The Master-Server daemon</description>
							<group>System Environment/Daemons</group>
							<version>${project.version}</version>
							<release>${build.number}</release>
							<packager>${packager.name} - ${packager.email}</packager>
							<distribution>${packager.distribution}</distribution>
							<license>Apache License 2.0</license>
							<requires>
								<require>java >= ${maven.compiler.target}</require>
							</requires>
							<mappings>
								<mapping>
									<directory>/usr/local/ecpds/master/conf</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												src/main/resources/master/conf</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/master/sh</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0755</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												src/main/resources/master/sh</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/master/lib</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												${project.basedir}/third-party</location>
										</source>
										<source>
											<location>
												${project.build.directory}/ecmwf-${project.version}-${build.number}.jar</location>
										</source>
									</sources>
								</mapping>
							</mappings>
						</configuration>
					</execution>
					<execution>
						<id>rpm-ecpds-monitor</id>
						<phase>package</phase>
						<goals>
							<goal>rpm</goal>
						</goals>
						<configuration>
							<name>ecpds-monitor</name>
							<summary>Monitor-Server</summary>
							<description>The Monitor-Server daemon</description>
							<group>System Environment/Daemons</group>
							<version>${project.version}</version>
							<release>${build.number}</release>
							<packager>${packager.name} - ${packager.email}</packager>
							<distribution>${packager.distribution}</distribution>
							<license>Apache License 2.0</license>
							<requires>
								<require>java >= ${maven.compiler.target}</require>
							</requires>
							<mappings>
								<mapping>
									<directory>/usr/local/ecpds/monitor/conf</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												src/main/resources/monitor/conf</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/monitor/sh</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0755</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												src/main/resources/monitor/sh</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/monitor/lib</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												${project.basedir}/third-party</location>
										</source>
										<source>
											<location>
												${project.build.directory}/ecmwf-${project.version}-${build.number}.jar</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>
										/usr/local/ecpds/monitor/htdocs/webapps</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												${project.build.directory}/ecmwf-${project.version}-${build.number}.war</location>
										</source>
									</sources>
								</mapping>
							</mappings>
						</configuration>
					</execution>
					<execution>
						<id>rpm-ecpds-mover</id>
						<phase>package</phase>
						<goals>
							<goal>rpm</goal>
						</goals>
						<configuration>
							<name>ecpds-mover</name>
							<summary>Mover-Server</summary>
							<description>The Mover-Server daemon</description>
							<group>System Environment/Daemons</group>
							<version>${project.version}</version>
							<release>${build.number}</release>
							<packager>${packager.name} - ${packager.email}</packager>
							<distribution>${packager.distribution}</distribution>
							<license>Apache License 2.0</license>
							<requires>
								<require>java >= ${maven.compiler.target}</require>
							</requires>
							<mappings>
								<mapping>
									<directory>/usr/local/ecpds/mover/conf</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												src/main/resources/mover/conf</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/mover/sh</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0755</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												src/main/resources/mover/sh</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/mover/lib</directory>
									<directoryIncluded>true</directoryIncluded>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												${project.basedir}/third-party</location>
										</source>
										<source>
											<location>
												${project.build.directory}/ecmwf-${project.version}-${build.number}.jar</location>
										</source>
									</sources>
								</mapping>
								<mapping>
									<directory>/usr/local/ecpds/mover/jni</directory>
									<filemode>0644</filemode>
									<dirmode>0755</dirmode>
									<sources>
										<source>
											<location>
												${project.build.directory}/libsocketoptions.so</location>
										</source>
									</sources>
								</mapping>

							</mappings>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
